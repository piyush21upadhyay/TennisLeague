<?xml version="1.0" encoding="utf-8"?><!--	City Golf League	Application Configuration	Created by 12-am.com--><application_configuration>	<name>City Golf League</name>	<description></description>	<version>1.0</version>	<build>1</build>	<url>http://www.citygolfleague.com</url>		<preload>		<!-- FONTS -->		<item>			<id>fonts</id>			<name>ApplicationFonts</name>			<type>font</type>			<description>Fonts to use in application</description>			<url>Fonts.swf</url>		</item>				<!-- CSS -->		<item>			<id>stylescss</id>			<name>styles</name>			<type>css</type>			<description>CSS to use for application.</description>			<url>styles.css</url>		</item>				<!-- SOUND -->		<item>			<id>swing</id>			<name>golfswing</name>			<type>sound</type>			<description>Golf swing sound</description>			<url>swing.mp3</url>		</item>		<item>			<id>cityrollover</id>			<name>cityRollover</name>			<type>sound</type>			<description>Rollover sound</description>			<url>cityRollover.mp3</url>		</item>				<!-- IMAGES -->		<item>			<id>introscreen</id>			<name>trophy</name>			<type>image</type>			<description>Intro page with trophy</description>			<url>trophy.jpg</url>		</item>		<item>			<id>bg_1</id>			<name>background</name>			<type>image</type>			<description>Background image</description>			<url>bg_1.jpg</url>		</item>		<item>			<id>bg_2</id>			<name>background</name>			<type>image</type>			<description>Background image</description>			<url>bg_2.jpg</url>		</item>		<item>			<id>bg_3</id>			<name>background</name>			<type>image</type>			<description>Background image</description>			<url>bg_3.jpg</url>		</item>		<item>			<id>bg_4</id>			<name>background</name>			<type>image</type>			<description>Background image</description>			<url>bg_4.jpg</url>		</item>
		<item>
			<id>demo</id>
			<name>Demo screenshot</name>
			<type>image</type>
			<description>Screen shot for demo panel</description>
			<url>demo_screen.jpg</url>
		</item>						<!-- XML -->		<item>			<id>contentxml</id>			<name>content</name>			<type>xml</type>			<description>Data for content.</description>			<url>content.xml</url>		</item>		<item>			<id>cityxml</id>			<name>city</name>			<type>service</type>			<description>Data for cities.</description>			<url>choosecity.html?format=xml</url>		</item>			</preload>		</application_configuration>